void main(){
    // membaut variabel dengan tipe data
    String name = "Novaland Sihotang";
    int umur = 21;

    // membuat veriabel dengan kata kunci var
    var alamat = "Padang Bulan, Medan";

    // mencetak variabel
    print("Nama saya $nama. Umur $umur tahun.");
    print("Alamat: $alamat");
}
